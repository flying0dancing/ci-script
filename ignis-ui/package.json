{
  "name": "fcr-engine",
  "version": "2.0.0",
  "scripts": {
    "build": "ng build",
    "build:prod": "ng build --prod",
    "e2e": "ng e2e",
    "lint": "npm run lint:code-quality",
    "lint:code-format": "prettier --check \"src/**/*{.ts,.js,.json,.css,.scss,.html}\"",
    "lint:code-quality": "ng lint",
    "lint:staged": "pretty-quick --fix",
    "mocks": "node mocks/server",
    "mocks:watch": "nodemon mocks/server.js",
    "ng": "ng",
    "protractor": "ng e2e -s false",
    "protractor:ci": "npm run protractor -- --no-progress --config=protractor-ci.conf.js",
    "serve": "ng serve --proxyConfig proxy.conf.json --aot --source-map=true",
    "serve:e2e": "node server.js",
    "serve:prod": "npm run serve:e2e -- -o",
    "start": "npm-run-all -p -r \"serve -- {@}\" mocks --",
    "start:e2e": "npm-run-all -p -r serve:e2e mocks",
    "start:prod": "npm-run-all -p -r serve:prod mocks",
    "test": "jest",
    "test:dev": "jest --watch",
    "test:ci": "npm run test -- --bail --silent --noStackTrace"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.0.0",
    "@angular/cdk": "8.0.1",
    "@angular/core": "8.0.0",
    "@angular/forms": "8.0.0",
    "@angular/common": "8.0.0",
    "@angular/router": "8.0.0",
    "@angular/compiler": "8.0.0",
    "@angular/material": "8.0.1",
    "@angular/material-moment-adapter": "8.0.1",
    "@angular/platform-browser": "8.0.0",
    "@angular/platform-browser-dynamic": "8.0.0",
    "@ngrx/effects": "^8.0.1",
    "@ngrx/entity": "^8.0.1",
    "@ngrx/router-store": "^8.0.1",
    "@ngrx/store": "^8.0.1",
    "@ngrx/store-devtools": "^8.0.1",
    "ag-grid": "18.1.2",
    "ag-grid-angular": "18.1.0",
    "ag-grid-enterprise": "18.1.1",
    "classlist.js": "1.1.20150312",
    "core-js": "2.5.7",
    "d3": "^5.9.2",
    "d3-dag": "^0.2.5",
    "downloadjs": "1.4.7",
    "jasmine-spec-reporter": "~4.2.1",
    "ngrx-store-logger": "0.2.2",
    "normalize.css": "8.0.0",
    "moment": "2.24.0",
    "roboto-fontface": "^0.8.0",
    "hammerjs": "2.0.8",
    "material-icons": "^0.3.1",
    "rxjs": "6.5.2",
    "web-animations-js": "github:angular/web-animations-js#release_pr208",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular/cli": "^8.0.2",
    "@angular/compiler-cli": "^8.0.0",
    "@angular/language-service": "8.0.0",
    "@types/jest": "23.3.1",
    "@types/node": "8.9.5",
    "codelyzer": "^5.0.1",
    "deep-freeze": "0.0.1",
    "jest": "23.4.2",
    "jest-junit": "5.1.0",
    "jest-preset-angular": "6.0.0",
    "morgan": "^1.9.1",
    "multer": "1.3.1",
    "ng-packagr": "^5.1.0",
    "npm-run-all": "4.1.3",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.0",
    "protractor": "5.3.2",
    "ts-node": "5.0.1",
    "tslint": "5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-consistent-codestyle": "1.13.3",
    "typescript": "3.4.5"
  }
}
